1 客户端检查更新

	GET /api/rc/v1/version_manage/latest?device_type=android&app_type=app&area_code=3500000000   // device_type:设备类型-android ios;  app_type:app类型- app wxmp
	req{

	}
	resp {
		"success": true,
		"message": "操作成功",
		"data": {
			"id": "800f39ce77d8437fa496b1120c4f9e77",	// 记录唯一id
		    "app_type": "app",							// 客户端类型 app:河长客户端 wxmp:公众号客户端
		    "device_type": "android",					// 设备类型 android ios
		    "version_number": "1.1.0",					// 版本号
		    "create_time": "2017-08-13T22:07:52.098Z",	// 创建时间
		    "update_time": "2017-08-13T22:07:52.098Z",	// 更新时间
		    "description": "test",						// 描述
		    "download_url": "http://localhost:80011011",// apk下载地址
		    "is_constraint": true,						// 是否强制更新
			"version_code":6							// 版本代号  (不同类型的值会有重复 由后端生成)

		}
	}
2 版本管理-->添加记录
	POST /api/rc/v1/version_manage
	req {
		"app_type": "app",					// 客户端类型 app:河长客户端 wxmp:公众号客户端
		"device_type": "android",			// 设备类型 android ios
		"version_number": "1.1.0",			// 版本号
		"description": "test",				// 描述
		"download_url": "1011",				// apk下载地址
		"is_constraint": true,				// 是否强制更新
		"area_code":"3500000000"            // 地区编号
    }
    resp {
		"success": true,
		"message": "操作成功",
		"data": []
    }

3 版本管理-->更新记录
	PUT	/api/rc/v1/version_manage/${id}    // id  记录id
	req {
		"app_type": "app",					// 客户端类型 app:河长客户端 wxmp:公众号客户端
		"device_type": "android",			// 设备类型 android ios
		"version_number": "1.1.0",			// 版本号
		"description": "test",				// 描述
		"download_url": "1011",				// apk下载地址
		"is_constraint": true,				// 是否强制更新
		"area_code":"3500000000"            // 地区编码

    }
	resp {
		"success": true,
		"message": "操作成功",
		"data": []
    }
4 版本管理-->删除记录 (包括删除和批量删除)
	DELETE	/api/rc/v1/version_manage
	req {
		"id_list":["f55500851349447f98e9415057fb6de5","faa19cb548a1492f9cc43ee5953fec02"]	// 对应记录的id
	}
	resp {
		"success": true,
		"message": "操作成功",
		"data": []
    }
